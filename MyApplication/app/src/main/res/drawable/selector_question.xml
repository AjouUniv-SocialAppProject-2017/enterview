<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- selector가 눌렸을때 -->
    <item android:state_pressed="true" android:drawable="@drawable/rounded_question_press"/>
    <item android:state_checked="true" android:drawable="@drawable/rounded_question_press"/>
    <item android:state_selected="true" android:drawable="@drawable/rounded_question_press"/>
    <item android:state_focused="true" android:drawable="@drawable/rounded_question_press"/>

    <!-- selector가 눌리지 않았을때 -->
    <item android:state_pressed="false" android:drawable="@drawable/rounded_question_normal"/>
    <item android:state_checked="false" android:drawable="@drawable/rounded_question_normal"/>
    <item android:state_selected="false" android:drawable="@drawable/rounded_question_normal"/>
    <item android:state_focused="false" android:drawable="@drawable/rounded_question_normal"/>
</selector>